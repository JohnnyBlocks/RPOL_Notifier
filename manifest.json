{
    "manifest_version": 2,

    "name": "RPOL Notifier",
    "description": "Notifies you of your RPOL RSS alerts",
    "version": "1.0",

    "permissions": [
        "storage",
        "http://rpol.net/"
    ],
    
    "options_page": "Options.html",
    
    "browser_action": {
        "default_icon":  "RPOL.png",
        "default_popup": "GameList.html"
    }
}